model Jurisdiction {
	@@schema("private")

	id String @db.Uuid @id @default(dbgenerated("gen_random_uuid()"))

	/// Name of the jurisdiction
	name String
	@@index([name(sort: Asc)])

	/// The country code for the jurisdiction, e.g., "US" for United States
	countryCode String

	/// The region or state within the jurisdiction, if applicable
	/// For example, "CA" for California, "TX" for Texas, etc.
	regionCode String?

	@@unique([countryCode(sort: Asc), regionCode(sort: Asc)])

	parentJurisdictionId String? @db.Uuid
	@@index([parentJurisdictionId])
	parentJurisdiction Jurisdiction? @relation("JurisdictionHierarchy", fields: [parentJurisdictionId], references: [id])
	childJurisdictions Jurisdiction[] @relation("JurisdictionHierarchy")

	validPartnerPromos PartnerPromo[] @relation("PartnerPromoValidJurisdictions")
	excludedPartnerPromos PartnerPromo[] @relation("PartnerPromoExcludedJurisdictions")

	users User[] @relation("UsersInJurisdiction")
}

