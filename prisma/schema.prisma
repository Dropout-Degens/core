generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["relationJoins", "typedSql"]
}

datasource db {
    provider     = "postgresql"
    url          = env("DATABASE_URL")
    directUrl    = env("DATABASE_DIRECT_URL")
    relationMode = "foreignKeys"
    schemas      = ["private", "public"]
}
